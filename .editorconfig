[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=4

[{.babelrc,.stylelintrc,.eslintrc,*.json,*.jsb3,*.bowerrc,*.jsb2}]
indent_style=space
indent_size=2

[*.java]
indent_style=tab
tab_width=4

[tslint.json]
indent_style=space
indent_size=2

[{*.ddl,*.sql}]
indent_style=space
indent_size=2

[*.less]
indent_style=space
indent_size=2

[*.scss]
indent_style=space
indent_size=2

[{*.yml,*.yaml}]
indent_style=space
indent_size=2

[*.go]
indent_style=tab
tab_width=8

